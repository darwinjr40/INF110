program main;
uses
  UCadena;

procedure PTiene2Pares;
var n : cardinal;
    res : boolean;
begin
  while True do
  begin
    write('ingrese un numero: ');
    readln(n);
    res :=  Tiene2Pares(n);
    writeln(res);
  end;
end;


procedure prueba;
var r : string;
    dato : byte;
begin
  while True do
  begin
    write('ingresa un numero: ');
    readln(dato);
    r := Romano2(dato);
    writeln(r);
  end;

end;

//restar octal---------------------------------------------------------
procedure restar_octal;
var r : string;
    a,b : integer;
begin
  while True do
  begin
    write('ingresa un numero a: ');
    readln(a);
    write('ingresa un numero b: ');
    readln(b);
    writeln(Restar(a, b));
  end;

end;

//main principal
begin
//   prueba;
  restar_octal();
  readln;
end.
